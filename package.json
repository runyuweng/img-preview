{
  "name": "img-preview",
  "version": "0.1.3",
  "description": "Img preview component for react",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wengwengweng/img-preview.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "dev": "cd ./example && webpack-dev-server --hot",
    "build": "babel lib -d dist --ignore spec.js,test.js",
    "test": "jest -u",
    "lint": "eslint ./lib/* ./example/*",
    "check": "node ./tools/check.js"
  },
  "pre-commit": [
    "check",
    "build"
  ],
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "collectCoverageFrom": [
      "lib/**/*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-minify-simplify": "^0.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^23.5.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "style-loader": "^0.19.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "bugs": {
    "url": "https://github.com/wengwengweng/img-preview/issues"
  },
  "homepage": "https://github.com/wengwengweng/img-preview#readme",
  "directories": {
    "example": "example"
  },
  "dependencies": {}
}
